@model IEnumerable<Test2.Models.OrdenPago>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Consumo de Web Service Órdenes de Pago de una Sucursal, por Moneda</h2>
<h3>Ejemplo. Banco 3, Sucursal 1, Moneda = Soles</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodBanco)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodSucursal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Moneda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CodBanco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodSucursal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Moneda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRegistro)
        </td>
    </tr>
}

</table>
